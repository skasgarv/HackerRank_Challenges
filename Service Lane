#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main(){
    int n,min;
    int t;
    cin >> n >> t;
    vector<int> width(n);
    for(int width_i = 0;width_i < n;width_i++){
       cin >> width[width_i];
    }
    for(int a0 = 0; a0 < t; a0++){
        int i;
        int j;
        cin >> i >> j;
        if(i<0 || j<0){
            return 0;
        }
        min = width[i];
        for(int x=i;x<=j;x++){
            if(min>width[x]){
                min = width[x];
            }
        }
        cout<<min<<endl;
    }
    return 0;
}
